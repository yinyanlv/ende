{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./typings"
    ],
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "downlevelIteration": true
  }
}
